{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "MEDIAWIKI_HAS_ALREADY_DATABASE",
          "value": "true"
        }
      ],
      "essential": true,
      "image": "hywiki/mediawiki:v0",
      "links": ["memcached"],
      "mountPoints": [
        {
          "containerPath": "/ct",
          "sourceVolume": "_Config"
        },
        {
          "containerPath": "/usr/src",
          "sourceVolume": "Files"
        }
      ],
      "name": "mediawiki",
      "memory": 128
    },
    {
      "essential": true,
      "image": "nginx:latest",
      "links": ["mediawiki"],
      "mountPoints": [
        {
          "containerPath": "/etc/nginx/conf.d/default.conf",
          "sourceVolume": "_HttpDefault_Conf"
        },
        {
          "containerPath": "/etc/nginx/nginx.conf",
          "sourceVolume": "_HttpNginx_Conf"
        },
        {
          "containerPath": "/usr/src",
          "sourceVolume": "Files"
        }
      ],
      "name": "http",
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        },
        {
          "containerPort": 443,
          "hostPort": 443
        }
      ]
    },
    {
      "environment": [
        {
          "name": "PARSOID_DOMAIN_mediawiki",
          "value": "http://http/api.php"
        }
      ],
      "essential": true,
      "image": "thenets/parsoid:0.10.0",
      "name": "parsoid",
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8081
        }
      ]
    },
    {
      "essential": true,
      "image": "memcached:1-alpine",
      "name": "memcached",
      "memory": 128
    }
  ],
  "volumes": [
    {
      "host": {
        "sourcePath": "/var/app/current/http/default.conf"
      },
      "name": "_HttpDefault_Conf"
    },
    {
      "host": {
        "sourcePath": "/var/app/current/http/nginx.conf"
      },
      "name": "_HttpNginx_Conf"
    },
    {
      "host": {
        "sourcePath": "files"
      },
      "name": "Files"
    },
    {
      "host": {
        "sourcePath": "/var/app/current/config"
      },
      "name": "_Config"
    }
  ]
}
